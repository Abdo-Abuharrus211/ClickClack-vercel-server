{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/docs/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/swagger-ui/(.*)",
            "dest": "/node_modules/swagger-ui-dist/$1"
        }
    ]
}
